    "react-native-web": "^0.8.6",
    "realm": "^2.12.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-jest": "23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-web": "^0.8.6",
